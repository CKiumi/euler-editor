{
  "name": "euler-editor",
  "private": true,
  "version": "0.2.6",
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "e2e": "playwright test",
    "e2e:update": "playwright test --update-snapshots",
    "coverage": "vitest run --coverage",
    "test:all": "tsc --noEmit && eslint . && vitest && playwright test",
    "lint": "tsc --noEmit && eslint .",
    "build": "tsc && vite build",
    "wasm": "cd ./engine && wasm-pack build --target web && cd ../",
    "preview": "vite preview",
    "pub": "yarn pack && mv euler-editor*.tgz ../euler-note/pkg"
  },
  "dependencies": {
    "euler-tex": "./pkg/euler-tex-v0.3.0.tgz",
    "euler-engine": "./pkg/euler-engine-v0.0.6.tgz"
  },
  "devDependencies": {
    "@playwright/test": "^1.21.1",
    "@types/katex": "^0.14.0",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "c8": "^7.11.2",
    "eslint": "^8.14.0",
    "happy-dom": "^3.1.0",
    "katex": "^0.15.3",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vitest": "^0.17.0"
  }
}
